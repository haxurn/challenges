{
  "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "lib": ["esnext"],
      "baseUrl": ".",
      "paths": {
          "@api/src/*": ["src/*"],
          "@api/bot/*": ["bot/*"],
      },
  }
}